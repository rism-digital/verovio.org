            <score>
               <scoreDef>
                  <staffGrp>
                     <staffDef n="1" lines="5" clef.shape="G" clef.line="2" key.sig="11s" />
                  </staffGrp>
               </scoreDef>
               <section>
                  <measure right="invis">
                     <staff n="1">
                        <layer n="1">
