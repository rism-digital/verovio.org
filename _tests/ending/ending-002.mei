<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="5.0">
   <meiHead>
      <fileDesc>
         <titleStmt>
            <title>Repeats and endings: sections and expansion lists</title>
         </titleStmt>
         <pubStmt>
            <date>2017-04-18</date>
         </pubStmt>
         <notesStmt>
            <annot>Verovio transforms humdrum sections *&gt; to MEI "section" elements, additionally transferring the expansion list *&gt;[A,B] to MEI
					"expansion".</annot>
         </notesStmt>
      </fileDesc>
      <encodingDesc>
         <appInfo>
            <application version="unknown">
               <name>Verovio</name>
            </application>
         </appInfo>
      </encodingDesc>
   </meiHead>
   <music>
      <body>
         <mdiv>
            <score>
               <scoreDef>
                  <staffGrp>
                     <staffDef n="1" lines="5" clef.shape="G" clef.line="2" key.sig="0" meter.count="4" meter.unit="4" />
                  </staffGrp>
               </scoreDef>
               <section>
                  <measure n="1">
                     <staff n="1">
                        <layer n="1">
                           <note dur="1" oct="4" pname="c">
                              <accid accid.ges="n" />
                           </note>
                        </layer>
                     </staff>
                     <tempo staff="1" tstamp="1.000000">Minuet</tempo>
                  </measure>
                  <ending n="1">
                     <measure n="2">
                        <staff n="1">
                           <layer n="1">
                              <note dur="1" oct="4" pname="d">
                                 <accid accid.ges="n" />
                              </note>
                           </layer>
                        </staff>
                     </measure>
                  </ending>
                  <ending n="2">
                     <measure right="rptend" n="3">
                        <staff n="1">
                           <layer n="1">
                              <note dur="1" oct="4" pname="e">
                                 <accid accid.ges="n" />
                              </note>
                           </layer>
                        </staff>
                        <dir place="below" staff="1" tstamp="1.000000">fine</dir>
                     </measure>
                  </ending>
                  <scoreDef key.sig="1f" />
                  <measure left="rptstart" n="4">
                     <staff n="1">
                        <layer n="1">
                           <note dur="1" oct="4" pname="f">
                              <accid accid.ges="n" />
                           </note>
                        </layer>
                     </staff>
                     <tempo staff="1" tstamp="1.000000">Trio</tempo>
                  </measure>
                  <measure right="rptend" n="5">
                     <staff n="1">
                        <layer n="1">
                           <note dur="1" oct="4" pname="g">
                              <accid accid.ges="n" />
                           </note>
                        </layer>
                     </staff>
                     <dir place="below" staff="1" tstamp="1.000000">minuet da capo</dir>
                  </measure>
               </section>
            </score>
         </mdiv>
      </body>
   </music>
</mei>
