   <meiHead>
      <fileDesc>
         <titleStmt>
            <title>Left/right repeats at line ends</title>
         </titleStmt>
         <pubStmt>
            <date>2017-04-27</date>
         </pubStmt>
         <notesStmt>
            <annot>If a measure with a right repeat is adjacent to a measure with a left repeat, Verovio renders a single thick barline and
					correctly splits the right/left repeat if a line break occurs between the two measures.</annot>
         </notesStmt>
      </fileDesc>
      <encodingDesc>
         <appInfo>
            <application version="unknown">
               <name>Verovio</name>
            </application>
         </appInfo>
      </encodingDesc>
   </meiHead>
