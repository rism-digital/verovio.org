   <music>
      <body>
         <mdiv>
            <score>
               <scoreDef key.mode="major" key.sig="0">
                  <staffGrp n="1">
                     <staffDef n="1" lines="5" clef.shape="G" clef.line="2" />
                  </staffGrp>
               </scoreDef>
               <section>
                  <measure label="Rests without dots">
                     <staff n="1">
                        <layer n="1">
                           <rest label="Long rest" dur.ges="4" dur="long" />
                           <rest label="Double whole rest" dur.ges="4" dur="breve" />
                           <rest label="Whole rest" dur.ges="4" dur="1" />
                           <rest label="Half rest" dur.ges="4" dur="2" />
                           <rest label="Quarter rest" dur.ges="4" dur="4" />
                           <rest label="Eighth rest" dur.ges="4" dur="8" />
                           <rest label="Sixteenth rest" dur.ges="4" dur="16" />
                           <rest label="Thirty-second rest" dur.ges="4" dur="32" />
                           <rest label="Sixty-fourth rest" dur.ges="4" dur="64" />
                           <rest label="Hundred twenty-eighth rest" dur.ges="4" dur="128" />
                           <rest label="Two hundred fifty-sixth rest" dur.ges="4" dur="256" />
                           <rest label="Five hundred twelfth rest" dur.ges="4" dur="512" />
                           <rest label="One thousand twenty-fourth rest" dur.ges="4" dur="1024" />
                        </layer>
                     </staff>
                  </measure>
                  <measure right="dbl" label="Rests with dots">
                     <staff n="1">
                        <layer n="1">
                           <rest label="Dotted whole rest" dots="1" dur.ges="4" dur="1" />
                           <rest label="Dotted half rest" dots="1" dur.ges="4" dur="2" />
                           <rest label="Dotted quarter rest" dots="1" dur.ges="4" dur="4" />
                           <rest label="Dotted eighth rest" dots="1" dur.ges="4" dur="8" />
                           <rest label="Dotted sixteenth rest" dots="1" dur.ges="4" dur="16" />
                           <rest label="Dotted thirty-second rest" dots="1" dur.ges="4" dur="32" />
                           <rest label="Dotted sixty-fourth rest" dots="1" dur.ges="4" dur="64" />
                           <rest label="Dotted hundred twenty-eighth rest" dots="1" dur.ges="4" dur="128" />
                           <rest label="Dotted two hundred fifty-sixth rest" dots="1" dur.ges="4" dur="256" />
                           <rest label="Dotted five hundred twelfth rest" dots="1" dur.ges="4" dur="512" />
                           <rest label="Dotted one thousand twenty-fourth rest" dots="1" dur.ges="4" dur="1024" />
                        </layer>
                     </staff>
                  </measure>
               </section>
            </score>
         </mdiv>
      </body>
   </music>
