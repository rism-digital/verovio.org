<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
   <meiHead>
      <fileDesc>
         <titleStmt>
            <title>Articulation placement and cross-staff beams</title>
            <respStmt>
               <persName role="editor">Laurent Pugin</persName>
               <persName role="encoder">Craig Sapp</persName>
            </respStmt>
         </titleStmt>
         <pubStmt>
            <date isodate="2021-01-27">2021-01-27</date>
            <pubPlace>
               <ref target="https://github.com/rism-digital/verovio/issues/957" />
            </pubPlace>
         </pubStmt>
         <seriesStmt>
            <title>Verovio test suite</title>
         </seriesStmt>
         <notesStmt>
            <annot />
         </notesStmt>
      </fileDesc>
      <encodingDesc>
         <appInfo>
            <application version="3.2.0" label="1">
               <name>Verovio</name>
            </application>
         </appInfo>
      </encodingDesc>
   </meiHead>
   <music>
      <body>
         <mdiv>
            <score>
               <scoreDef>
                  <staffGrp bar.thru="true" symbol="brace">
                     <label />
                     <staffDef n="1" lines="5" clef.shape="G" clef.line="2" meter.count="3" meter.unit="4" />
                     <staffDef n="2" lines="5" clef.shape="F" clef.line="4" meter.count="3" meter.unit="4" />
                  </staffGrp>
               </scoreDef>
               <section>
                  <measure>
                     <staff n="1">
                        <layer n="1">
                           <beam>
                              <note dur="16" oct="3" pname="b" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" oct="3" pname="a" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" oct="3" pname="g" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" oct="3" pname="f" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                           </beam>
                           <beam>
                              <note dur="16" oct="3" pname="b" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" oct="3" pname="a" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" staff="2" oct="3" pname="g" stem.dir="up" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" staff="2" oct="3" pname="f" stem.dir="up" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                           </beam>
                           <beam staff="2">
                              <note dur="16" staff="2" oct="3" pname="b" stem.dir="up" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" staff="2" oct="3" pname="a" stem.dir="up" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" staff="2" oct="3" pname="g" stem.dir="up" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                              <note dur="16" staff="2" oct="3" pname="f" stem.dir="up" accid.ges="n">
                                 <artic artic="stacc" place="above" />
                              </note>
                           </beam>
                        </layer>
                     </staff>
                     <staff n="2">
                        <layer n="1">
                           <mRest ploc="b" oloc="2" />
                        </layer>
                     </staff>
                  </measure>
               </section>
            </score>
         </mdiv>
      </body>
   </music>
