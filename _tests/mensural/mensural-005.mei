   <music>
      <body>
         <mdiv>
            <score>
               <scoreDef>
                  <staffGrp>
                     <staffDef n="1" notationtype="mensural" lines="5">
                        <clef shape="C" line="1" />
                     </staffDef>
                  </staffGrp>
               </scoreDef>
               <section>
                  <staff n="1">
                     <layer n="1">
                        <note dur="semibrevis" oct="4" pname="f">
                           <accid accid="s" />
                        </note>
                        <note dur="semibrevis" oct="4" pname="b">
                           <accid accid="n" />
                        </note>
                     </layer>
                  </staff>
               </section>
            </score>
         </mdiv>
      </body>
   </music>
