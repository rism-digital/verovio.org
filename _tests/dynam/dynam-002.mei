<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="4.0.0">
   <meiHead>
      <fileDesc>
         <titleStmt>
            <title>Dynamic example (2)</title>
         </titleStmt>
         <pubStmt>
            <date isodate="2017-05-04">2017-05-04</date>
         </pubStmt>
         <seriesStmt>
            <title>Verovio test suite</title>
         </seriesStmt>
         <notesStmt>
            <annot>Dynamics for which the text does not correspond to a SMuFL code are rendered in italic. The text is then left-aligned.</annot>
         </notesStmt>
