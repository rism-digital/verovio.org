<mei xmlns="http://www.music-encoding.org/ns/mei" meiversion="5.0">
   <meiHead>
      <fileDesc>
         <titleStmt>
            <title>Collision between layers</title>
         </titleStmt>
         <pubStmt>
            <date>2017-05-17</date>
         </pubStmt>
         <seriesStmt>
            <title>Verovio test suite</title>
         </seriesStmt>
         <notesStmt>
            <annot>Verovio avoids collisions between notes and accidentals belonging to different layers on the same staff.</annot>
         </notesStmt>
      </fileDesc>
      <encodingDesc>
         <appInfo>
            <application version="2.0.0" label="2">
               <name>Verovio</name>
            </application>
         </appInfo>
      </encodingDesc>
   </meiHead>
   <music>
      <body>
         <mdiv>
            <score>
               <scoreDef>
                  <staffGrp>
                     <staffDef n="1" lines="5" clef.shape="G" clef.line="2" />
                  </staffGrp>
               </scoreDef>
               <section>
                  <measure right="end" n="0">
                     <staff n="1">
                        <layer n="1">
                           <note dur="4" oct="4" pname="b">
                              <accid accid="f" />
                           </note>
                           <note dur="4" oct="4" pname="e">
                              <accid accid="x" />
                           </note>
                           <note dur="4" oct="4" pname="a">
                              <accid accid="f" />
                           </note>
                           <note dur="4" oct="4" pname="e">
                              <accid accid="ff" />
                           </note>
                        </layer>
                        <layer n="2">
                           <note dur="4" oct="4" pname="g">
                              <accid accid="s" />
                           </note>
                           <note dur="4" oct="4" pname="d">
                              <accid accid="s" />
                           </note>
                           <note dur="4" oct="4" pname="g">
                              <accid accid="f" />
                           </note>
                           <note dur="4" oct="4" pname="d">
                              <accid accid="f" />
                           </note>
                        </layer>
                     </staff>
                  </measure>
               </section>
            </score>
         </mdiv>
      </body>
   </music>
