   <define name="speaker">
      <element name="speaker">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Contains a specialized form of heading or label, giving the name of one or more speakers in a dramatic text or fragment.</a:documentation>
         <zeroOrMore>
            <choice>
               <text/>
               <ref name="model.textPhraseLike.limited"/>
               <ref name="model.editLike"/>
               <ref name="model.transcriptionLike"/>
            </choice>
         </zeroOrMore>
         <ref name="att.common.attributes"/>
         <ref name="att.facsimile.attributes"/>
