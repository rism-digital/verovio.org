         <ref name="system"/>
      </choice>
   </define>
   <define name="system">
      <element name="system">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">A system in a page-based encoding</a:documentation>
         <zeroOrMore>
            <choice>
               <ref name="model.secbLike"/>
               <ref name="model.measureLike"/>
               <ref name="model.staffLike"/>
               <ref name="model.scoreDefLike"/>
               <ref name="model.milestoneLike"/>
            </choice>
         </zeroOrMore>
         <ref name="att.systems.attributes"/>
         <ref name="att.common.attributes"/>
         <ref name="att.coordinated.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="model.secbLike">
      <choice>
         <ref name="secb"/>
      </choice>
   </define>
   <define name="secb">
      <element name="secb">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(section beginning) – A milestone element indicating the start of a new section in page-based MEI.</a:documentation>
         <empty/>
         <ref name="att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="model.mdivbLike">
      <choice>
         <ref name="mdivb"/>
      </choice>
   </define>
   <define name="mdivb">
      <element name="mdivb">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(mdiv beginning) – A milestone element indicating the start of a new mdiv in page-based MEI.</a:documentation>
         <empty/>
         <ref name="att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="model.milestoneLike">
      <choice>
         <ref name="milestone"/>
         <ref name="milestoneEnd"/>
      </choice>
   </define>
   <define name="milestone">
      <element name="milestone">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(milestone) – A generic milestone element that can be used as anchor in an MEI file.</a:documentation>
         <empty/>
         <ref name="att.common.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="milestoneEnd">
      <element name="milestoneEnd">
         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">(milestone end) – A milestone element that can be used to indicate the end of an arbitrary feature in MEI.</a:documentation>
         <empty/>
         <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
                  id="mei-milestoneEnd-check_milestoneEnd-constraint-rule-183">
            <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                      xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                      xmlns="http://www.tei-c.org/ns/1.0"
                      context="mei:milestoneEnd">
               <sch:assert role="error" test="exists(@startid)">milestoneEnd needs a @startid attribute.</sch:assert>
            </sch:rule>
         </pattern>
         <ref name="att.common.attributes"/>
         <ref name="att.startId.attributes"/>
         <empty/>
      </element>
   </define>
   <define name="att.coord.x1.attributes">
      <ref name="att.coord.x1.attribute.coord.x1"/>
   </define>
   <define name="att.coord.x1.attribute.coord.x1">
      <optional>
         <attribute name="coord.x1">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gives coordinates for the left side of a feature.</a:documentation>
            <data type="decimal">
               <param name="minInclusive">0</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.coord.x2.attributes">
      <ref name="att.coord.x2.attribute.coord.x2"/>
   </define>
   <define name="att.coord.x2.attribute.coord.x2">
      <optional>
         <attribute name="coord.x2">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gives coordinates for the right side of a feature.</a:documentation>
            <data type="decimal">
               <param name="minInclusive">0</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.coord.y1.attributes">
      <ref name="att.coord.y1.attribute.coord.y1"/>
   </define>
   <define name="att.coord.y1.attribute.coord.y1">
      <optional>
         <attribute name="coord.y1">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">gives y coordinates for the top left of a feature.</a:documentation>
            <data type="decimal">
               <param name="minInclusive">0</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="att.coord.lr.attributes">
      <ref name="att.coord.x1.attributes"/>
      <ref name="att.coord.x2.attributes"/>
   </define>
   <define name="att.surface.attributes">
      <ref name="att.surface.attribute.surface"/>
   </define>
   <define name="att.surface.attribute.surface">
      <optional>
         <attribute name="surface">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">contains a reference to a surface element</a:documentation>
            <ref name="data.URI"/>
         </attribute>
      </optional>
   </define>
   <define name="att.note.ges.tab.attribute.tab.course">
      <optional>
         <attribute name="tab.course">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Records which course is to be played.</a:documentation>
            <ref name="data.COURSENUMBER"/>
         </attribute>
      </optional>
   </define>
   <define name="att.note.ges.tab.attribute.tab.fret">
      <optional>
         <attribute name="tab.fret">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Records which course is to be played.</a:documentation>
            <ref name="data.FRETNUMBER"/>
         </attribute>
      </optional>
   </define>
   <define name="att.course.log.attribute.tuning.standard">
      <optional>
         <attribute name="tuning.standard">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Indicates a tuning standard for the course.</a:documentation>
            <ref name="data.COURSETUNING"/>
         </attribute>
      </optional>
   </define>
   <start>
      <choice>
         <ref name="mei"/>
         <ref name="meiHead"/>
         <ref name="meiCorpus"/>
         <ref name="music"/>
      </choice>
   </start>
</grammar>
