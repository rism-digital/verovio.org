         <ref name="att.common.attributes"/>
         <ref name="att.facsimile.attributes"/>
         <ref name="att.ambNote.log.attributes"/>
         <ref name="att.ambNote.vis.attributes"/>
         <ref name="att.ambNote.ges.attributes"/>
         <ref name="att.ambNote.anl.attributes"/>
