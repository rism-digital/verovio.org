         </attribute>
         <optional>
            <attribute name="context">
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Circumstances in which the element appears, an XPath expression.</a:documentation>
               <data type="string"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="occurs">
