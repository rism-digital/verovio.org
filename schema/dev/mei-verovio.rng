         <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Range of a voice, instrument or piece.</a:documentation>
         <ref name="ambNote"/>
         <ref name="ambNote"/>
         <ref name="att.common.attributes"/>
         <ref name="att.facsimile.attributes"/>
         <ref name="att.ambitus.anl.attributes"/>
         <ref name="att.ambitus.ges.attributes"/>
         <ref name="att.ambitus.log.attributes"/>
         <ref name="att.ambitus.vis.attributes"/>
         <ref name="att.metadataPointing.attributes"/>
